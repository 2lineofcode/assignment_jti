{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".idea": true,
        ".dart_**": true,
        ".firebase": true,
        ".flutter-**": true,
        "linux": true,
        "android": false,
        "windows": true,
        "macos": true,
        "ios": false,
        "web": true,
        "build": true,
        "supabase": true
    },
    "explorerExclude.backup": {},

    "dart.lineLength": 200,
    "workbench.editor.enablePreview": true,

    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata, analysis_options.yaml, flutter_launcher_icons.yaml",
        "README.md":"*.md",
        "*.dart": "${capture}.g.dart",
        "firebase.json": "firebase.json, .firebaserc",
        ".env": ".env, .env.dev, .env.prod",
    },
}